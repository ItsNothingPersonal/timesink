{
  "root": "libs/feature-v20-types",
  "sourceRoot": "libs/feature-v20-types/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/feature-v20-types/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/feature-v20-types"],
      "options": {
        "jestConfig": "libs/feature-v20-types/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:util", "scope:v20", "scope:client"]
}
